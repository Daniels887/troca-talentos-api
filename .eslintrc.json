{
  "env": {
    "es2020": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "class-methods-use-this": "off",
    "import/no-unresolved": "off",
    "no-unused-vars": "off",
    "import/extensions": "off",
    "no-console": "off",
    "import/prefer-default-export": "off"
  }
}
